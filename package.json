{
	"name": "kyleharper.co.uk",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "npm run build:css && remix vite:build",
		"build:css": "tailwindcss -m -i ./app/tailwind.css -o app/tailwind-o.css",
		"dev": "concurrently \"npm run dev:css\" \"remix vite:dev\"",
		"dev:css": "tailwindcss -w -i ./app/tailwind.css -o app/tailwind-o.css",
		"check": "biome check --apply ./*",
		"start": "remix-serve ./build/server/index.js",
		"typecheck": "tsc"
	},
	"dependencies": {
		"@mdx-js/rollup": "^3.0.1",
		"@remix-run/node": "^2.9.2",
		"@remix-run/react": "^2.9.2",
		"@remix-run/serve": "^2.9.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"recma-export-filepath": "^1.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@flydotio/dockerfile": "^0.5.7",
		"@remix-run/dev": "^2.9.2",
		"@tailwindcss/typography": "^0.5.13",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"concurrently": "^8.2.2",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5",
		"vite": "^5.2.12",
		"vite-tsconfig-paths": "^4.3.2"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
